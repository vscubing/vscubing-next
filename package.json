{
  "name": "vscubing-next",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build --turbo",
    "preview": "next build --turbo && cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/ && bun .next/standalone/server.js",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate-no-legacy": "./scripts/migrate-no-legacy.sh",
    "db:generate": "drizzle-kit generate",
    "db:custom": "drizzle-kit generate --custom --name",
    "db:drop": "drizzle-kit drop",
    "db:local": "./scripts/start-database.sh",
    "db:studio": "drizzle-kit studio",
    "check": "bun format:check; bun lint:check; bun type:check; bun run test",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint:check": "next lint",
    "lint:write": "next lint --fix",
    "type:check": "tsc --noEmit",
    "test": "bun test"
  },
