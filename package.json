{
  "name": "vscubing-next",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:seed": "bun run ./src/server/internal/seed",
    "db:custom": "drizzle-kit generate --custom --name",
    "db:drop": "drizzle-kit drop",
    "db:local": "./start-database.sh && drizzle-kit studio",
    "db:studio": "drizzle-kit studio",
    "check": "bun format:check && bun lint:check && bun type:check && bun test",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint:check": "next lint",
    "lint:write": "next lint --fix",
    "type:check": "tsc --noEmit",
    "test": "bun test",
    "vendor": "./install-vendor.sh",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy"
  },
  "dependencies": {
