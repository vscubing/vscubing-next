    "@t3-oss/env-nextjs": "^0.12.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.1",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.1",
    "@trpc/tanstack-react-query": "^11.0.1",
    "@types/bun": "^1.2.8",
    "@vscubing/cubing": "0.55.0-vscubing.11",
